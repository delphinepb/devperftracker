datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Analysis {
  id       Int      @id @default(autoincrement())
  url      String
  score    Int
  date     DateTime @default(now())
  metrics  Json?
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  role     String
}